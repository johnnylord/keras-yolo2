        "batch_size":           32,
        "learning_rate":        1e-4,
        "nb_epochs":            50,

        "object_scale":         5.0,
        "no_object_scale":      0.5,
        "coord_scale":          2.0,
        "class_scale":          1.0,

        "pretrained_weights":   "models/yolo_weights.h5",
        "saved_weights_name":   "models/yolo_weights.h5",
        "debug":                true
    }
